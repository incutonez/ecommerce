{
  "version": 2,
  "buildCommand": "cd ../.. && npm run api:build",
  "installCommand": "npm install --prefix=../..",
  "outputDirectory": "dist",
  "framework": null,
  "builds": [
    {
      "src": "src/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ]
    }
  ]
}
